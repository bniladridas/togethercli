DYLIB_EXT = .dylib
DYLIB_LD = $(CC)
DYLIB_LDFLAGS = -dynamiclib -Wl,-headerpad_max_install_names -undefined dynamic_lookup# $(DYLIB_LTO)
## used for libR as a shared library
DYLIB_LINK = $(DYLIB_LD) $(DYLIB_LDFLAGS) $(LDFLAGS)
DYLIB_LTO = $(CFLAGS) $(CPICFLAGS) 
ECHO = echo
ECHO_C = \c
ECHO_N = 
ECHO_T = 
FFLAGS = -g -O2 
FLIBS =  -L/opt/homebrew/Cellar/gcc/15.1.0/lib/gcc/current/gcc/aarch64-apple-darwin24/15 -L/opt/homebrew/Cellar/gcc/15.1.0/lib/gcc/current/gcc -L/opt/homebrew/Cellar/gcc/15.1.0/lib/gcc/current -lemutls_w -lheapt_w -lgfortran -lquadmath
## needed for some earlier Solaris compilers
FLIBS_IN_SO =  -L/opt/homebrew/Cellar/gcc/15.1.0/lib/gcc/current/gcc/aarch64-apple-darwin24/15 -L/opt/homebrew/Cellar/gcc/15.1.0/lib/gcc/current/gcc -L/opt/homebrew/Cellar/gcc/15.1.0/lib/gcc/current -lemutls_w -lheapt_w -lgfortran -lquadmath
FPICFLAGS = -fPIC
FPIEFLAGS = -fPIE
FC = gfortran
GETWD = /bin/pwd
GZIP = --best
INSTALL = /usr/bin/install -c
INSTALL_DATA = ${INSTALL} -m 644
INSTALL_PROGRAM = ${INSTALL}
INSTALL_SCRIPT = ${INSTALL}
INSTALL_DIR = ${INSTALL} -d
LDFLAGS = -L/opt/homebrew/opt/xz/lib -L/opt/homebrew/opt/pcre2/lib
LIBINTL=   -Wl,-framework -Wl,CoreFoundation
LIBM = 
LIBR0 = 
LIBR1 = 
LIBR =  
## These are the libs which the final R.bin/libR is linked against.
## Many of these used to be optional: ICU libs still are.
## There may be platform-specific ones, e.g.
## -lrt -ldl on Linux, -lnsl -lsocket -lrt -lsunmath -ldl -liconv on Solaris.
LIBS = -L/opt/homebrew/Cellar/pcre2/10.45/lib -lpcre2-8 -llzma -lbz2 -lz -licucore -ldl -lm -liconv
LIBnn = lib
LIBTOOL = $(SHELL) $(top_builddir)/libtool
## AFAICS unused
LN_S = ln -s
MAIN_CFLAGS = 
MAIN_FFLAGS = 
MAIN_LD = $(CC)# $(CFLAGS) $(CPICFLAGS) 
MAIN_LDFLAGS =   # -Wl,-bE:$(top_builddir)/etc/R.exp
MAIN_LINK = $(MAIN_LD) $(MAIN_LDFLAGS) $(LIBR0) $(LDFLAGS)
## need this for bootstrapping
MKINSTALLDIRS = /bin/sh $(top_srcdir)/src/scripts/mkinstalldirs.in
NOTANGLE = notangle
R_ARCH = 
R_DYLIB_EXT = .dylib
R_FRAMEWORK_DIR = $(prefix)/R.framework
R_GZIPCMD = /usr/bin/gzip
## needed for AIX only
# R_HOME = $(top_builddir)
R_OPENMP_CFLAGS = 
## if ever used, need to check that this is compatible with C OpenMP
## and add it to ALL_FFLAGS{,_LO}
R_OPENMP_FFLAGS = -fopenmp
R_OSTYPE = unix
R_PKGS = $(R_PKGS_BASE)  $(R_PKGS_RECOMMENDED)
R_PLATFORM = aarch64-apple-darwin24.4.0
## set on AIX, formerly for old glibc (-D__NO_MATH_INLINES)
R_XTRA_CFLAGS =  
R_XTRA_CPPFLAGS =  -I. -I$(top_builddir)/src/include -I$(top_srcdir)/src/include
## used for gfortran >= 7 in R 3.6.1 - 4.2.x
R_XTRA_FFLAGS = 
RANLIB = ranlib
READLINE_LIBS = -lreadline 
SED = /usr/bin/sed
SHELL = /bin/sh
SHLIB_EXT = .so
SHLIB_CFLAGS = 
SHLIB_FFLAGS = 
SHLIB_LD = $(CC)
SHLIB_LDFLAGS = -dynamiclib -Wl,-headerpad_max_install_names -undefined dynamic_lookup# $(SHLIB_LTO)
SHLIB_LINK = $(SHLIB_LD) $(SHLIB_LDFLAGS) $(LIBR0) $(LDFLAGS)
SHLIB_LTO = $(CFLAGS) $(CPICFLAGS)
STRIP_SHARED_LIB = strip -x
STRIP_STATIC_LIB = strip -S
TAR = /usr/bin/tar
USE_NLS = yes
X_CFLAGS = 
X_LIBS = 
X_PRE_LIBS = 
X_EXTRA_LIBS = 
YACC = bison -y

## MAIN_[CF]FLAGS are used on some systems when profiling R.
ALL_CFLAGS = $(R_XTRA_CFLAGS) $(R_OPENMP_CFLAGS) $(MAIN_CFLAGS) $(CFLAGS)
ALL_CPPFLAGS = $(R_XTRA_CPPFLAGS) $(CPPFLAGS) $(DEFS)
ALL_FFLAGS = $(R_XTRA_FFLAGS) $(MAIN_FFLAGS) $(FFLAGS)

## For use in src/{appl,main} when R is a shared library, src/modules/lapack
## and src/extra/blas when internal BLAS is built as a shared library
## SHLIB_CFLAGS and SHLIB_FFLAGS are apparently currently unused
ALL_CFLAGS_LO = $(R_XTRA_CFLAGS) $(R_OPENMP_CFLAGS) $(CPICFLAGS) $(SHLIB_CFLAGS) $(CFLAGS)
ALL_FFLAGS_LO = $(R_XTRA_FFLAGS) $(FPICFLAGS) $(SHLIB_FFLAGS) $(FFLAGS)
ALL_FFLAGS_PIE = $(R_XTRA_FFLAGS) $(FPIEFLAGS) $(SHLIB_FFLAGS) $(FFLAGS)

.SUFFIXES:
.SUFFIXES: .c .f .m .d .o

.c.o:
	$(CC) $(ALL_CPPFLAGS) $(ALL_CFLAGS) -c $< -o $@
.c.d:
	@echo "making $@ from $<"
	@$(CC) -MM $(ALL_CPPFLAGS) $< > $@
.f.o:
	$(FC) $(ALL_FFLAGS) -c $< -o $@

prefix = /usr/local
exec_prefix = ${prefix}
datarootdir = ${prefix}/share
## only used for installing 'R'.
bindir = ${exec_prefix}/bin
## not used
datadir = ${datarootdir}
## used for 'rhome' and installation of standalone Rmath
libdir = ${exec_prefix}/${LIBnn}
## used for man page
mandir = ${datarootdir}/man
## used for installation of standalone Rmath headers
includedir = ${prefix}/include

rhome = ${libdir}/R
rsharedir = ${rhome}/share
rincludedir = ${rhome}/include
rdocdir = ${rhome}/doc

## Overrides for installing R as a framework (macOS).
#FW_VERSION = 
#rhome = $(R_FRAMEWORK_DIR)/Versions/$(FW_VERSION)/Resources
## Note: scripts install doesn't like bindir and Rexecbindir to be the same
#bindir = $(rhome)
#mandir = $(rhome)/share/man
#libdir = $(rhome)/lib

Rexecbindir = $(rhome)/bin
Rexecbindir2 = $(rhome)/bin/exec$(R_ARCH)
Rexeclibdir = $(rhome)/lib$(R_ARCH)
## FIXME:
## Alternatively, we could try to set pkglibdir = $(rhome)/lib when
## switching to automake.
## </FIXME>
Rexecmodulesdir = $(rhome)/modules$(R_ARCH)
